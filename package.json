{
  "name": "coffee-app",
  "version": "0.0.1",
  "description": "A fun and useful coffee app",
  "keywords": [
    "capacitor",
    "ionic",
    "stencil",
    "mobile"
  ],
  "scripts": {
    "lint": "npm run eslint && npm run prettier -- --check",
    "eslint": "eslint src/ --ext .js,.ts,.jsx,.tsx",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write",
    "prettier": "prettier \"**/*.ts\"",
    "lint.fix": "lint -- --fix",
    "buildProd": "stencil build --prod",
    "buildDebug": "stencil build",
    "syncNative": "npx cap sync",
    "buildNativeDebug": "npm run buildDebug && npm run buildDebug && npx cap sync",
    "buildNativeProd": "npm run buildProd && npm run buildProd && npx cap sync",
    "openios": "npx cap open ios",
    "openandroid": "npx cap open android",
    "start": "stencil build --dev --watch --serve",
    "serve": "stencil serve --root www",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@capacitor/android": "latest",
    "@capacitor/app": "latest",
    "@capacitor/browser": "latest",
    "@capacitor/camera": "latest",
    "@capacitor/core": "latest",
    "@capacitor/device": "latest",
    "@capacitor/haptics": "latest",
    "@capacitor/ios": "latest",
    "@capacitor/keyboard": "latest",
    "@capacitor/network": "latest",
    "@capacitor/splash-screen": "latest",
    "@capacitor/status-bar": "latest",
    "@capacitor/storage": "latest",
    "@ionic/core": "5.8.0",
    "autonumeric": "4.6.0",
    "firebase": "9.0.2",
    "rxfire": "6.0.2",
    "rxjs": "7.3.0"
  },
  "devDependencies": {
    "@capacitor/cli": "latest",
    "@ionic/eslint-config": "latest",
    "@ionic/prettier-config": "latest",
    "@stencil/core": "latest",
    "@types/jest": "26.0.24",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "eslint": "7.17.0",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "prettier": "2.3.2",
    "puppeteer": "10.0.0",
    "typescript": "4.4.2"
  },
  "author": "Jay Cambron",
  "license": "MIT",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "prettier": "@ionic/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cambronjay/coffee-app.git"
  },
  "bugs": {
    "url": "https://github.com/cambronjay/coffee-app/issues"
  },
  "homepage": "https://github.com/cambronjay/coffee-app"
}
